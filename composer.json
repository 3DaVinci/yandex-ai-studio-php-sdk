{
  "name": "3davinci/yandex-ai-studio-php-sdk",
  "minimum-stability": "stable",
  "version": "0.1.0",
  "description": "Yandex AI Studio PHP SDK",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aslangeri Mokaev",
      "email": "aslangery@3davinci.ru"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/http-client": "^7.4.x-dev",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "AIStudio\\": "src/AIStudio"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AIStudio\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}